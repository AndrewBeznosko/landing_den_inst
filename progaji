 function add_ss_visitor_uid() {
     window.ss_try++
     $('form').each(function(i,e) {
        if($(e).find('[name="visitor_uid"]').length == 0)
            $(e).append(`<input type="hidden" name="visitor_uid" value="${window.visitor_uid}">`);
    });
